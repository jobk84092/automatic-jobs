{
   "action": {
      "default_icon": "cheerpjAR_128x128_gray.png",
      "default_popup": "popup.html",
      "default_title": "CheerpJ Applet Runner Disabled"
   },
   "background": {
      "service_worker": "bg.js"
   },
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
   },
   "description": "Run Java applets without installing Java",
   "icons": {
      "128": "cheerpjAR_128x128.png",
      "32": "cheerpjAR_32x32.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "CheerpJ Applet Runner",
   "optional_host_permissions": [ "http://*/", "https://*/" ],
   "permissions": [ "scripting", "activeTab", "storage", "declarativeNetRequestWithHostAccess" ],
   "storage": {
      "managed_schema": "storage_schema.json"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2025.6",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "cheerpj/*", "init_applet.js" ]
   } ]
}
