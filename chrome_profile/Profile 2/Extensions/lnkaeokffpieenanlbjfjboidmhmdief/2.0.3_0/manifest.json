{
   "action": {
      "default_icon": {
         "128": "assets/icon128.png",
         "16": "assets/icon16.png",
         "48": "assets/icon48.png"
      },
      "default_popup": "popup/index.html",
      "default_title": "__MSG_extName__"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "content_scripts": [ {
      "js": [ "lib/import-html2canvas.min.js", "content-scripts/import-main.js", "lib/import-gtag.js" ],
      "matches": [ "https://*/*", "http://*/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": {
      "object-src": "self",
      "script-src": "self",
      "unsafe-eval": [ "https://*/*", "http://*/*", "chrome-extension://*/*" ]
   },
   "default_locale": "en",
   "description": "__MSG_extDescription__",
   "host_permissions": [ "https://*/*", "http://*/*" ],
   "icons": {
      "128": "assets/icon128.png",
      "16": "assets/icon16.png",
      "48": "assets/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "__MSG_extName__",
   "optional_host_permissions": [ "https://*/*", "http://*/*", "chrome-extension://*/*" ],
   "permissions": [ "desktopCapture", "activeTab", "tabs", "contextMenus", "storage", "scripting" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.3",
   "web_accessible_resources": [ {
      "matches": [ "https://*/*", "http://*/*", "chrome-extension://*/*" ],
      "resources": [ "assets/**/*.png", "assets/**/*.gif", "aboutUs/index.html", "/edit/edit.html", "assets/**/*.css", "assets/fonts/*.*", "lib/gtag.js" ]
   }, {
      "matches": [ "https://*/*", "http://*/*" ],
      "resources": [ "*-*.js", "lib/html2canvas.min.js", "content-scripts/main.js", "lib/gtag.js" ]
   } ]
}
