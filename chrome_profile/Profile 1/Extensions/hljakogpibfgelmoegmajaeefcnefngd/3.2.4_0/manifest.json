{
   "action": {
      "default_icon": "icons/icon19.png",
      "default_popup": "src/browser_action/browser_action.html",
      "default_title": "__MSG_browser_action_title__"
   },
   "author": "Eclat Engineering Pvt. Ltd.",
   "background": {
      "service_worker": "src/bg/service_worker.js",
      "type": "module"
   },
   "content_scripts": [ {
      "js": [ "src/content/inject-icon.js", "src/content/search-keyword-analytics.js" ],
      "matches": [ "*://*/*" ]
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'  ; object-src 'self'; default-src 'self'  https://*.myloft.xyz https://*.remotexs.cn  https://*.eclateng.com  https://*.remotexs.co;     style-src 'self' https://fonts.googleapis.com 'unsafe-inline';font-src 'self' https://fonts.googleapis.com https://fonts.gstatic.com data:; "
   },
   "default_locale": "en",
   "description": "__MSG_manifest_description__",
   "externally_connectable": {
      "matches": [ "https://*.myloft.xyz/*" ]
   },
   "homepage_url": "https://app.myloft.xyz/",
   "host_permissions": [ "*://*/*" ],
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "110",
   "name": "MyLOFT",
   "options_ui": {
      "open_in_tab": false,
      "page": "src/options/options.html"
   },
   "permissions": [ "webRequest", "webRequestAuthProvider", "scripting", "activeTab", "proxy", "cookies", "idle", "notifications", "storage", "alarms" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.2.4",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "src/domain-rules/domain-rules.html", "/icons/*", "/src/notifiers/*", "/assets/*", "/src/content/findDoi.css" ]
   } ]
}
